---
interface Props {
  title: string;
}

const { title } = Astro.props;
const currentYear = new Date().getFullYear();
---

<footer class="site-footer">
  <div class="container">
    <div class="footer-content">
      <div class="logo">{title}</div>
      <p class="footer-text">© {currentYear} Все права защищены</p>
    </div>
  </div>
</footer>

